{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataFactoryRaionDojo"
		},
		"AzureFunctionGymdeskSync_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'AzureFunctionGymdeskSync'"
		},
		"AzureSqlDatabase_PowerBI_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase_PowerBI'"
		},
		"BlobStoragePowerBI_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStoragePowerBI'"
		},
		"HttpSharePointService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'HttpSharePointService'"
		},
		"AzureFunctionGymdeskSync_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://gymdesksync.azurewebsites.net"
		},
		"AzureSqlDatabase_PowerBI_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-svr-rd-1.database.windows.net"
		},
		"AzureSqlDatabase_PowerBI_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "PowerBI"
		},
		"AzureSqlDatabase_PowerBI_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "manuel.brunner"
		},
		"HttpSharePointService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mindrevolutiongmbh527.sharepoint.com/sites/FileserverRaionDojo"
		},
		"HttpSharePointService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "manuel.brunner@raion-dojo.ch"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataFactoryRaionDojo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureFunctionGymdeskSync_functionKey": {
						"value": "[parameters('AzureFunctionGymdeskSync_functionKey')]"
					},
					"AzureSqlDatabase_PowerBI_password": {
						"value": "[parameters('AzureSqlDatabase_PowerBI_password')]"
					},
					"BlobStoragePowerBI_connectionString": {
						"value": "[parameters('BlobStoragePowerBI_connectionString')]"
					},
					"HttpSharePointService_password": {
						"value": "[parameters('HttpSharePointService_password')]"
					},
					"AzureFunctionGymdeskSync_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('AzureFunctionGymdeskSync_properties_typeProperties_functionAppUrl')]"
					},
					"AzureSqlDatabase_PowerBI_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_PowerBI_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_PowerBI_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_PowerBI_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_PowerBI_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_PowerBI_properties_typeProperties_userName')]"
					},
					"HttpSharePointService_properties_typeProperties_url": {
						"value": "[parameters('HttpSharePointService_properties_typeProperties_url')]"
					},
					"HttpSharePointService_properties_typeProperties_userName": {
						"value": "[parameters('HttpSharePointService_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}