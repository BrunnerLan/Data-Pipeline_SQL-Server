{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CSVFilesSource_Members",
						"type": "DatasetReference"
					},
					"name": "CSVSourceCheckInCode",
					"description": "Remove empty lines"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "FilterEmptyCheckInCode"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> CSVSourceCheckInCode",
				"CSVSourceCheckInCode filter(ERROR_FUNCTION('!isNull([Check-in code]) __NEW_LINE_N__')) ~> FilterEmptyCheckInCode"
			]
		}
	}
}