{
	"name": "CopyBlobToSQL",
	"properties": {
		"activities": [
			{
				"name": "CopyDataToSQL1",
				"description": "Copies data from Blob Storage to SQL database <members>",
				"type": "Copy",
				"state": "Inactive",
				"onInactiveMarkAs": "Succeeded",
				"dependsOn": [],
				"policy": {
					"timeout": "0.00:15:00",
					"retry": 3,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "TRUNCATE TABLE dbo.members;",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": false,
							"treatBooleanAsNumber": false,
							"dateFormat": "dd.MM.yyyy",
							"dateTimeFormat": "dd.MM.yyyy HH:mm",
							"culture": "de-CH"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "CSVFilesSource_Members",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SQLDatabaseSink_Members",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "GymDeskToBlobStorage",
				"description": "Time-controlled function",
				"type": "AzureFunctionActivity",
				"state": "Inactive",
				"onInactiveMarkAs": "Succeeded",
				"dependsOn": [],
				"policy": {
					"timeout": "0.00:15:00",
					"retry": 3,
					"retryIntervalInSeconds": 30,
					"secureOutput": true,
					"secureInput": true
				},
				"userProperties": [],
				"typeProperties": {
					"functionName": "gymdeskSyncTrigger",
					"method": "GET"
				},
				"linkedServiceName": {
					"referenceName": "AzureFunctionGymdeskSync",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "SwitchCopyData",
				"description": "=> Copy Data to SQL Server\n=> Copy Data to Sharepoint",
				"type": "Switch",
				"dependsOn": [
					{
						"activity": "GymDeskToBlobStorage",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"cases": []
				}
			}
		],
		"annotations": []
	}
}